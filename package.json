{
  "name": "gerencio-upgrade",
  "version": "0.0.0",
  "description": "ugrades a single service in gerenc.io",
  "main": "dist/index.js",
  "bin": {
    "gerencio-upgrade": "dist/cli.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "doc": "typedoc --excludeExternals --hideGenerator --exclude **/index.ts --out ./docs",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublish": "yarn build",
    "start": "ts-node -r tsconfig-paths/register src/cli.ts",
    "test": "NODE_ENV=test jest",
    "tsfmt": "tsfmt --no-tslint --no-vscode --verbose -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerencio/gerencio-upgrade-v3.git"
  },
  "keywords": [
    "gerencio",
    "upgrade",
    "gerencio.io"
  ],
  "author": "xDevel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gerencio/gerencio-upgrade-v3/issues"
  },
  "homepage": "https://github.com/gerencio/gerencio-upgrade-v3#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">= 5.3.0",
    "yarn": ">= 1.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -c tslint.json ",
      "tsfmt",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/download": "^6.2.3",
    "@types/fs-extra": "^5.0.4",
    "@types/js-yaml": "^3.11.2",
    "@types/request": "^2.48.1",
    "codelyzer": "^4.5.0",
    "dotenv": "^6.1.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "typescript-formatter": "^7.2.2"
  },
  "dependencies": {
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "download": "^7.1.0",
    "fs-extra": "^7.0.1",
    "fs-sync": "^1.0.6",
    "js-yaml": "^3.12.0",
    "request": "^2.88.0",
    "unzip2": "^0.2.5",
    "util": "^0.11.1",
    "write-yaml": "^1.0.0"
  }
}
